[run]
source = services
omit =
    */tests/*
    */__main__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abstract
    if 0:
    if False:

precision = 2
skip_covered = False

[html]
directory = htmlcov
